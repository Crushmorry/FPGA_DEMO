#include <stdio.h>
#include "xil_cache.h"
#include "xil_printf.h"
#include "xil_io.h"
int main()
{
	int i;
	char c='a';
	Xil_DCacheDisable();
	printf("AXI4 PL DDR TEST!\n\r");

	while(1)
	{
		scanf("%c",&c);
		printf("%c\r\n",c);
		if(c=='c')
		{
			printf("start\n\r");
			for(i = 0 ;i<4096;i+=4)
			{
				Xil_Out32(Xil_In32(0x10000000+i), 2048+i);
			}
			for(i=0;i<4096;i+=4)
			{
				printf("%d is %d\n",i,(int)Xil_In32(0x10000000+i));
			}
		}
	}
	return 0;
}
