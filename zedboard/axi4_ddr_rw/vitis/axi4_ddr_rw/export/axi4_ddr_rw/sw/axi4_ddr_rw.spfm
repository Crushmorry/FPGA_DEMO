<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="axi4_ddr_rw"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
axi4_ddr_rw
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="axi4_ddr_rw">
        <sdx:configuration sdx:name="axi4_ddr_rw"
                           sdx:displayName="axi4_ddr_rw"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>axi4_ddr_rw</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="axi4_ddr_rw/boot/axi4_ddr_rw.bif"
                            sdx:readme="axi4_ddr_rw/boot/generic.readme"
                            sdx:qemuArguments="axi4_ddr_rw/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="axi4_ddr_rw/boot/axi4_ddr_rw.bif"
                            sdx:readme="axi4_ddr_rw/boot/generic.readme"
                            sdx:qemuArguments="axi4_ddr_rw/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="axi4_ddr_rw/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="axi4_ddr_rw/standalone_domain/bspinclude/include"
                        sdx:bspConfig="axi4_ddr_rw/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
